{
  "name": "near-rpc-typescript",
  "version": "0.1.0",
  "description": "Automated TypeScript RPC client for NEAR Protocol",
  "type": "module",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "clean": "pnpm -r clean",
    "generate": "pnpm --filter @near-js/jsonrpc-types generate",
    "typecheck": "pnpm -r typecheck",
    "type-coverage": "typescript-coverage-report",
    "dev": "pnpm -r --parallel dev"
  },
  "devDependencies": {
    "@types/node": "^24.0.14",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitest/coverage-v8": "^1.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semantic-ui-react": "^2.1.5",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "typescript-coverage-report": "^1.1.1",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petersalomonsen/near-rpc-typescript.git"
  },
  "keywords": [
    "near",
    "protocol",
    "rpc",
    "typescript",
    "blockchain",
    "api",
    "client"
  ],
  "author": "",
  "license": "MIT"
}
