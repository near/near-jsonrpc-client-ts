{
  "name": "@near-js/jsonrpc-types",
  "version": "1.0.0",
  "description": "TypeScript types and Zod schemas for NEAR Protocol JSON-RPC API",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "prettier --check 'src/**/*.{ts,js}'",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate": "tsx ../../tools/codegen/generate.ts"
  },
  "dependencies": {
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/petersalomonsen/near-rpc-typescript.git",
    "directory": "packages/jsonrpc-types"
  },
  "keywords": [
    "near",
    "protocol",
    "rpc",
    "types",
    "typescript",
    "zod",
    "validation"
  ],
  "author": "",
  "license": "MIT"
}
